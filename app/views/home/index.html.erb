
  <h1>XTUNES</h1>

<table>
  <tr>
    <th>Song</th>
    <th>Artist</th>
    <th>Album</th>
    <th>Price</th>
    <th>Genre</th>

  </tr>

<% @songs.each do |song| %>
  <tr>
    <td><%= song.name %></td>
    <td><%= song.artist %></td>
    <td><%= song.album %></td>
    <td><%= song.price %></td>
    <td><%= song.genre %></td>
  </tr>
<% end %>
</table>