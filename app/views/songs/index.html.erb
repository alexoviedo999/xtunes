<h1>Listing Songs</h1>
<table>
  <tr>
    <th>Song</th>
    <th>Artist</th>
    <th>Album</th>
    <th>Price</th>
    <th>Genre</th>

  </tr>

<% @songs.each do |song| %>
  <tr>
    <td><%= song.name %></td>
    <td><%= song.artist %></td>
    <td><%= song.album %></td>
    <td><%= song.price %></td>
    <td><%= song.genre %></td>


    <td><%= link_to 'Show', song %></td>

    <td><%= link_to 'Edit', edit_song_path(song) %></td>
     <td><%= link_to 'Delete', song, method: :delete, data: {confirm: 'Are you sure?'} %></td>

  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Song', new_song_path %>
